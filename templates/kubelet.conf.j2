kind: KubeletConfiguration
apiVersion: kubelet.config.k8s.io/v1beta1
evictionHard:
    memory.available:  "128Mi"
authentication:
  anonymous:
    enabled: false
  x509:
    clientCAFile: "{{ kubelet_ca }}"
staticPodPath: {{ kubelet_static_pod_path }}
healthzBindAddress: 127.0.0.1
healthzPort: 10251
kubeletCgroups: "/systemd/system.slice"
tlsCertFile: "{{ kubelet_cert }}"
tlsPrivateKeyFile: "{{ kubelet_key }}"
failSwapOn: False
runtimeRequestTimeout: 15m
clusterDNS:
  - {{ kubernetes_dns_service_ip }}
clusterDomain: {{ kubernetes_cluster_domain }}
systemReserved:
  cpu: "{{ kubelet_system_reserved.cpu }}"
  memory: "{{ kubelet_system_reserved.memory }}"
systemReservedCgroup: "/system-reserved"
kubeReserved:
  cpu: "{{ kubelet_kube_reserved.cpu }}"
  memory: "{{ kubelet_kube_reserved.memory }}"
cgroupDriver: "systemd"
kubeReservedCgroup: "/kube-reserved"
